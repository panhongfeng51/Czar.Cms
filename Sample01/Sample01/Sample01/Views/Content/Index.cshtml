
@model ContentViewModel
@{
ViewData["Title"]="内容列表";

}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <div>@ViewData["Title"]</div>
        <table>
            <thead >
                <tr>
                    <td><input type="checkbox" /></td>
                    <td>序号</td>
                    <td>标题</td>
                    <td>内容</td>
                    <td>添加时间</td>
                </tr>

            </thead>
            @foreach(var item in Model.Contents){
            <tr>
                <td><input type="checkbox" /></td>
                <td>@item.Id</td>
                <td>@item.title</td>
                <td>@item.content</td>
                <td>@item.add_time</td>
            </tr>
            }
        </table>
    </div>
</body>
</html>
